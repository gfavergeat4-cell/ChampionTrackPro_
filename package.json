{
  "name": "championtrackpro",
  "version": "1.0.0",
  "private": true,
  "main": "index.js",
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "start": "expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "web:start": "expo start --web",
    "web:build": "expo export --platform web --output-dir web/dist",
    "web:build:meta": "npm run web:build && node scripts/inject-metadata.js",
    "og:generate": "node scripts/generate-og-image.js",
    "android:bundle": "npx expo export --platform android --output-dir android-bundle --clear",
    "android:gradle:clean": "cd android && gradlew clean",
    "android:assembleDebug": "cd android && gradlew assembleDebug",
    "android:assembleRelease": "cd android && gradlew assembleRelease"
  },
  "dependencies": {
    "@expo-google-fonts/cinzel": "^0.4.2",
    "@expo-google-fonts/rajdhani": "^0.4.1",
    "@expo/metro-runtime": "~6.1.2",
    "@react-native-async-storage/async-storage": "^1.24.0",
    "@react-native-community/slider": "5.0.1",
    "@react-navigation/bottom-tabs": "^7.1.8",
    "@react-navigation/native": "^7.1.17",
    "@react-navigation/native-stack": "^7.3.26",
    "axios": "^1.12.2",
    "date-fns": "^4.1.0",
    "expo": "54.0.20",
    "expo-font": "~14.0.9",
    "expo-linear-gradient": "~15.0.7",
    "expo-status-bar": "~3.0.8",
    "firebase": "^10.14.1",
    "ical": "^0.8.0",
    "ical-expander": "^3.2.0",
    "luxon": "^3.7.2",
    "node-ical": "^0.21.0",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-native": "0.81.5",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-get-random-values": "^2.0.0",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "react-native-web": "^0.21.0"
  },
  "expo": {
    "entryPoint": "./index.js"
  },
  "devDependencies": {
    "@react-native-community/cli": "^20.0.2",
    "@react-native-community/cli-platform-android": "^20.0.2",
    "@react-native/metro-config": "^0.81.0",
    "metro-config": "^0.81.0",
    "sharp": "^0.33.0"
  }
}
